import { println } from "std::io";

function main(): void {
  let values: i64[5] = [1, 2, 3, 4, 5];

  for (let i = 0; i < values.length; i++) {
    println(values[i]);
  }

  for (let value of values) {
    println(value);
  }

  let mut i: i64 = 0;
  while (i < values.length) {
    println(values[i]);
    i++;
    // ++i;
    // i += 1;
    // i = i + 1;
  }
}
