function println(value: unknown): void;

function main(): void {
  let values: i64[5] = [1, 2, 3, 4, 5];

  for (let i = 0; i < 5; i++) {
    println(values[i]);
  }

  for (let value of values) {
    println(value);
  }

  let mut i: i32 = 0;
  while (i < 5) {
    println(values[i as u32]);
    i++;
    // ++i;
    // i += 1;
    // i = i + 1;
  }
}
