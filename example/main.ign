import println from "std::io";
import toString from "std::string";

export function add(a: &i32, b: i32): i32 {
  return *a + b;
}

function main(): void {
  let mut value: i32 = 42;
  let mut var: i32 = 10;

  value += var;

  let result: i32 = add(&value, var);
  println(toString(result));
  return;
}
