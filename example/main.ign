function println(value: string, ...args: unknown[]): void;

record Person {
  name: string;
  age: i64;
  getName(): string;
}

function main(...args: string[]): void {
  let person: Person = {
    name: "John",
    age: 30,
    getName(): string {
      return this.name;
    },
  };

  if (person.getName() == "John") {
    println("person name is John");
  } else {
    println("person name is not John");
  }
}
