// Generic type alias with single type parameter
type Identity<T> = T;

// Generic type alias with multiple type parameters  
type First<A, B> = A;

record Box<T> {
    value: T;
}

// Type alias wrapping a generic record
type IntBox = Box<i32>;

// Type alias using generic type parameter as argument to another generic
type BoxAlias<T> = Box<T>;

function identity<T>(value: T): T {
    return value;
}

function main(): void {
    // Use Identity type alias
    let x: Identity<i32> = 42;
    
    // Use IntBox type alias
    let box: IntBox = Box { value: 100 };
    
    // Use BoxAlias type alias
    let boxAlias: BoxAlias<boolean> = Box { value: true };
    
    // Use First type alias
    let first: First<i32, boolean> = 10;
    
    // Use identity function
    let y: i32 = identity<i32>(x);
    
    return;
}
