// Test: Extern functions don't consume ownership
// Expected: OK - extern calls don't move variables

@implements(Drop)
record Owned {
    public id: i32;
    drop(&mut self): void { return; }
}

extern io {
    function print(s: Owned): void;
}

function main(): void {
    let a: Owned = Owned { id: 1 };

    // Extern call doesn't consume - variable can be reused
    io::print(a);
    io::print(a);
    io::print(a);

    return;
}
