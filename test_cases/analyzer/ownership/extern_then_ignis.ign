// Test: Extern call followed by Ignis call
// Expected: ERROR O0001 - use after move on third call

extern io {
    function print(s: string): void;
}

function consume(s: string): void {
    return;
}

function main(): void {
    let a: string = "hello";
    
    // Extern call - doesn't consume, a still valid
    io::print(a);
    
    // Ignis call - consumes, a is now moved
    consume(a);
    
    // This should error - a was moved
    io::print(a);
    
    return;
}
