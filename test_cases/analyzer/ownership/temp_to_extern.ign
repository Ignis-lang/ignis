// Test: Passing temporary to extern function
// Expected: OK - temps passed to extern are dropped after call
// Note: This tests runtime behavior (no leaks), not static analysis

@implements(Drop)
record Owned {
    public id: i32;
    drop(&mut self): void { return; }
}

extern io {
    function print(s: Owned): void;
}

function createOwned(): Owned {
    return Owned { id: 1 };
}

function main(): void {
    // Passing temp directly to extern - should be dropped after call
    io::print(createOwned());
    io::print(createOwned());

    return;
}
