// Test: Record implementing both Drop and Clone
// Expected: no errors

@implements(Drop, Clone)
record Buffer {
  size: i32;

  drop(&mut self): void {
    return;
  }

  clone(&self): Buffer {
    return Buffer { size: self.size };
  }
}

function main(): i32 {
  let b: Buffer = Buffer { size: 64 };
  let c: Buffer = b.clone();
  return 0;
}
