// Test: Record with correct Clone implementation
// Expected: no errors

@implements(Clone)
record Point {
  x: i32;
  y: i32;

  clone(&self): Point {
    return Point { x: self.x, y: self.y };
  }
}

function main(): i32 {
  let p: Point = Point { x: 1, y: 2 };
  let q: Point = p.clone();
  return 0;
}
