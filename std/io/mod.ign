// Ignis Standard Library - I/O Module
// Provides basic input/output functions

import stringConcat from "std::string";

extern function print(message: string): void;
extern function eprint(message: string): void;

export function println(message: string): void {
  print(stringConcat(message, "\n"));
}

export function eprintln(message: string): void {
  eprint(stringConcat(message, "\n"));
}

