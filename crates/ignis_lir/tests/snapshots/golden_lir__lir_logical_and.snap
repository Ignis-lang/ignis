=== LIR ===

fn both(a: bool, b: bool): bool {
  locals:
    %0: bool = a
    %1: bool = b
    %2: bool = _ [mut]

  entry_0:
    store %0, t0
    store %1, t1
    t2 = load %0 : bool
    store %2, t2
    br t2, sc_right_1, sc_merge_2

  sc_right_1:
    t3 = load %1 : bool
    store %2, t3
    goto sc_merge_2

  sc_merge_2:
    t4 = load %2 : bool
    ret t4
}
